{
    "name": "@kiro-lens/shared",
    "version": "1.0.0",
    "description": "Kiro Lens共通型定義パッケージ",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./types/*": {
            "types": "./dist/types/*.d.ts",
            "import": "./dist/types/*.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "files": [
        "dist/**/*"
    ]
}